{
	"exclude": [
		"dist/",
		"test/"
	],
	"compilerOptions": {
		"strict": true,
		"declaration": true,
		"declarationMap": true,
		"importHelpers": true,
		"pretty": true,
		"rootDir": "src",
		"outDir": "dist",
		"target": "es2021",
		"module": "commonjs",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"esModuleInterop": true
	}
}